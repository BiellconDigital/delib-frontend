marron = #7d6350

@font-face {
	font-family: 'Heldustry';
	font-style: normal;
	font-weight: normal;
	src:url('../fonts/HeldustryFT_Regular.ttf') format('truetype');
}

@font-face {
	font-family: 'mangal';
	font-style: normal;
	font-weight: normal;
	src:url('../fonts/mangal.ttf') format('truetype');
}

@font-face {
	font-family: 'mangalb';
	font-style: normal;
	font-weight: normal;
	src:url('../fonts/mangalb.ttf') format('truetype');
}

@font-face {
	font-family: 'Pacifico';
	font-style: normal;
	font-weight: normal;
	src:url('../fonts/Pacifico.ttf') format('truetype');
}

@font-face {
	font-family: 'Remachine';
	font-style: normal;
	font-weight: normal;
	src:url('../fonts/RemachineScript.ttf') format('truetype');
}

html
	overflow hidden

a
	color marron


.container-body
	position absolute
	top 0
	bottom 0 
	right 0 
	left 0
    width  100%
	height 100%
	max-height 100%
	overflow hidden

.wrapper	
	display block
	height 100%
	max-height 100%
	overflow-x hidden
	overflow-y auto

	position relative
	z-index 3
		 
.gap 
	height  40px

footer
	background marron
	color white
	padding 0.2em 0
	text-align center
	width 100%
	position absolute
	margin 0
	bottom -1px
	display block
	z-index 4
	height 40px
	right 0px
	line-height 40px
	h3
		position absolute
		font-size 14px
		font-weight normal
		margin auto
		width 100%
		text-align center
		z-index 5
	.container
		position absolute
		margin auto 1%
		z-index 6
		width 98%
		.links
			float left
			margin 6px 0
			width 12%
			height 26px
			line-height 25px
			z-index 6
			font-size 0.4em
			a
				color white
				font-weight normal
		.linea-separacion
			border-right 1px solid white

header
	
	.logo
		text-align center
		margin 3em auto auto auto
		width 190px
		a
			border none
			img
				width 190px
	.spacer		
		background marron
		display block
		height 1.5px
		margin 1em auto auto auto
		width 85%

.content
	padding-bottom 20px
	.contenido
		position relative
		.about
			top 100%
			height 40%
			color rgba(black,0.4)
			font-family "mangal"
			margin-left 7%
			margin-right 7%
			text-align center
			padding-bottom 0
			z-index 20
			h3
				color marron
				font-size 1.5em
				font-family "Pacifico"
				padding-bottom 35px
			p
				margin 0
				padding 0
		.informacion
			position relative
			margin-top -37%
			margin-left 7%
			margin-right 7%
			box-shadow inset 0 3.2em 0 0 rgba(grey, 0)
			color marron
			font-family "mangal"
			z-index 20
			h2
				padding 0
				margin 0
				color white
				font-size 1.2em
				text-indent 24px
			div
				background-color white
				padding 10px


		.banner
		.banner2
			position relative
			margin-top 1em
			overflow hidden
			height 440px
			width 1500px
			z-index 11
			img
				z-index 11
				height 540px
				margin -50px 0 0 -380px
		.text
			font-family "Remachine"
			font-size 4em
			margin auto
			overflow hidden
			position absolute
			height 70px
			padding 12px
			top 46%		
			width 100%
			color white
			text-align center					
			text-shadow 2px 2px 10px rgba(black, 0.8)
			z-index 99999
		.text2
			position absolute
			font-family "Remachine"
			font-size 4em
			margin auto
			overflow hidden
			padding 12px
			padding-top 19%
			height 50px
			width 100%
			color white
			text-align center					
			text-shadow 2px 2px 10px rgba(black, 0.8)
			z-index 99999


	.main-banner
		margin-top 1em
		overflow hidden
		img
			height 400px
			width 100%
	.contact
		margin auto
		margin-top 1.1em
		overflow hidden
		text-align center
		text-size 1em
		color marron
		font-size 0.8em
		background-color #EAF9F9
		.izq 
		.der
			display inline-block
			margin 2em
			p
				font-family "mangalb"
			h2
				font-size 1.7em
				font-weight normal
				font-family "Heldustry"

		.izq
			text-align right
			margin-right 0
			.direccion
				padding-right 38px
				background-image url(../img/amor-amar.png)
				background-repeat no-repeat
				background-position right

		.der
			text-align left	
			border-left solid 2px marron
			padding-left 2em

		.spacer		
			background marron
			display inline-block
			height 200px
	.contact-map
		#map_canvas
			width 100%
			height 320px
	
	.layerFondo
		margin-top 10px
		padding-top 25px
		position absolute
		background-image url(../img/registro_fondo.jpg)
		background-size 100% 100%
		color white
		font-family 'Heldustry'
		height 100%
		width 100%
		z-index 990
		font-size 1.2em
		vertical-align center	
		text-decoration none
		zoom 1
		h3
			font-size 1em
			font-weight normal
			margin 0 10%
			padding 4px 0
		hr
			margin 0 8%
			border 0
			height 1px
			background-color grey
			text-shadow inset 1px 1px rgba(grey, 0.5)
		.row-fluid
			.info
				position absolute
				margin auto
				color marron
				top 25%
				left 0
				right 0
				width 80%
				.info-seccion
					background-color white
					border-radius 9px
					padding 25px
					margin-bottom 6px




.layerOpacity
	background rgba(0,0,0,0.89)
	background transparent\9
	color white
	display none
	font-size 'Heldustry'
	height 90%
	position fixed
	width 100%
	z-index 990
	font-size 1em
	vertical-align center	
	text-decoration none
	zoom 1
	.lista
		position relative
		width 80%
		left 10%
		top 0%
		li
			padding 0.4em
			list-style-type none
			margin-top 1em
			border-bottom solid 0.1em
			border-color white
			
			input
				-moz-border-radius 15px
				border-radius 15px
				background-image url(../img/lupa-busqueda.png)
				background-repeat no-repeat
				background-position right
				text-indent 10em
				&:active
				&:focus 
					background-image none
	.minus
		position absolute
		bottom -30px
		right 0


nav
	margin-top 0
	position relative
	display none
	.menu
		position absolute
		margin-top 4.9em
		width 85%
	.info-head
		position absolute
		margin-left 1em
		margin-top 0.1em
		width 210px
		float none
		.logo-fb
			margin 0 23px 1px 0
			float left
		.nro-delivery
			background-image url(../img/fondo-num-telf-deli.png)
			width 121px
			height 23px
			margin 0 14px 1px 0
			text-indent 0.7em
			color white
			font-size 1.1em
			line-height 1.6em
			float left
		.text-delivery
			margin 0 14px 1px 0
			width 100px
			float left
			line-height 2.2em
			color marron
		.logo-delivery
			margin 0 4px 1px 0
			float left

	.carrito-header
		margin-right 1em
		float right
		margin-top 0.4em
		.carrito-header-cont
			float none
			color marron
			width 360px
			font-size 0.9em
			div
				float right
			a
				color marron
			.text-login
				float right
				width 350px
				text-align right
				margin-bottom 0.2em
				.name-user
					font-size 1em
					font-family "Pacifico"
			.carrito-logo
				float right
				background-image url(../img/carro-de-compra1.png)
				text-indent 2.1em
				font-size 0.8em
				line-height 1.8em
				width 40px
				height 26px
			input
				float right
				-moz-border-radius 6px
				border-radius 6px
				border none
				text-align left
				color white
				padding 3px
				width 136px
				background-image url(../img/lupa-busqueda-blanca.png)
				background-repeat no-repeat
				background-position 94% 50%
				background-color marron
				text-indent 1em
				margin-right 0.8em
				&:active
				&:focus 
					background-image none


